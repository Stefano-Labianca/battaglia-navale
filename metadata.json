{
    "gameRound": 0, // Segna il turno della partita - Numero naturale - > 0
    "next": 1, // Indica quale giocatore deve fare la sua mossa - Numero naturale - next = 1 -> Giocatore 2, next = 0 -> Giocatore 1
    "ended": 0, // Indica se la partita è terminata - Booleano - ended = False -> Partita ancora in corso, ended = True -> Partita terminata

    // Dato strutturato della tabella
    "Grid": {
        "values": "Array<Array<char>>", // Contiene informazioni relative alla mappa di gioco - Array bidimensionale di caratteri - Dimensioni row x column
        "row": 16, // Numero di righe della mappa - Numero naturale - row > 0 && row <= 16 
        "columns": 16 // Numero di colonne - NUmnero naturale - column > 0 && column <= 16
    },

    // Dato strutturato della nave
    "Ship": {
        "label": "$", // Carattere che identifica una nave - Carattere - /
        "sunk": 1, // Indica se una nave è stata affondata - Booleano - sunk = True -> Nave affondata, sunk = False -> Nave non affondata
        "size": 3, // Rappresenta lo spazio occupato della nave - Numero naturale - dimension >= 1 && dimension <= 5
        "range": "1-4-V", // Contiene il range di posizioni della nave e il suo orientamento (che può essere orizzontale o verticale) - Stringa - /
        "coords": "A-1_A-4" // Stringa che contiene le coordinate della nave - Stringa - /
    },

    // Dato struturato del giocatore
    "Player": {
        "id": 1, // Identifica un giocatore - Numero naturale - /
        "playground": "Grid", // Rappresenta la tabella del giocatore - Grid - /
        "heatMap": "Grid", // Contiene delle informazioni relative alle conoscenze della mappa dell'avversario - Grid - /
        "longShots": 3, // Indica il numero di colpi a largo raggio disponibili - Numero naturale - longShot >= 0 && longShot <= 3
        "radar": 3, // Indica il numero di utilizzi del radar ancora disponibili - Numero naturale - radar >= 0 && radar <= 3
        "airstrike": 1, // Indica il numero di utilizzi del bombardamento aereo ancora disponibili - Numero naturale - Utilizzabile dal turno 10, airstrike >= 0 && airstrike <= 1
        "availableShips": 15, // Rappresenta il numero di navi attaualmente disponibili al giocatore - Numero naturale - availableShips >= 0 && availableShips <= 15
        "ships": "Array<Ship>" // Contiene le navi del giocatore - Array di Ship - Dimensioni = 15
    }
}